type: datasource.sqlite
options:
  provider: DATA_PROVIDER_DYNAMIC

  entities:
    - default/photos

  query: |
    SELECT 
      id, 
      '$.uploadPhoto' AS uploadPhoto, 
      '$.title' AS title,
      '$.expireDate'AS expire,
      '$.description' AS description
    FROM [default/photos]
    WHERE expire > @currentMillis
    ORDER BY expire DESC
  queryParameters:
    currentMillis: =$string($millis())